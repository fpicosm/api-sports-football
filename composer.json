{
    "name": "fpicosm/api-sports-football",
    "description": "A Laravel/Lumen wrapper for https://www.api-football.com/ calls",
    "version": "1.0.0",
    "keywords": ["football", "api", "sports"],
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Picos",
            "email": "fpicosm@gmail.com",
            "homepage": "https://github.com/fpicosm",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "fpicosm@gmail.com",
        "docs": "https://api-sports.io/documentation/football/v3"
    },
    "require": {
        "illuminate/support": "^9.18",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/laravel": "^9.1"
    },
    "autoload": {
